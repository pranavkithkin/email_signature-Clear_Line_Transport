<svg width="600" height="80" viewBox="0 0 600 80" fill="none" xmlns="http://www.w3.org/2000/svg">
  <!-- Background gradient -->
  <defs>
    <linearGradient id="bgGradient" x1="0" y1="0" x2="600" y2="0">
      <stop offset="0%" style="stop-color:#2c3e50;stop-opacity:1" />
      <stop offset="50%" style="stop-color:#1a9b8e;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#2c3e50;stop-opacity:1" />
    </linearGradient>

    <linearGradient id="lineGradient" x1="0" y1="0" x2="600" y2="0">
      <stop offset="0%" style="stop-color:#ffffff;stop-opacity:0.2" />
      <stop offset="50%" style="stop-color:#ffffff;stop-opacity:0.8" />
      <stop offset="100%" style="stop-color:#ffffff;stop-opacity:0.2" />
    </linearGradient>
  </defs>

  <!-- Main background -->
  <rect width="600" height="80" fill="url(#bgGradient)"/>

  <!-- Top accent line -->
  <line x1="0" y1="2" x2="600" y2="2" stroke="url(#lineGradient)" stroke-width="2"/>

  <!-- Transport vehicles -->

  <!-- Truck 1 (Left) -->
  <g transform="translate(80, 25)">
    <rect x="0" y="15" width="35" height="18" rx="2" fill="white" opacity="0.9"/>
    <rect x="35" y="10" width="20" height="23" rx="2" fill="white" opacity="0.9"/>
    <circle cx="10" cy="35" r="4" fill="#2c3e50"/>
    <circle cx="28" cy="35" r="4" fill="#2c3e50"/>
    <circle cx="45" cy="35" r="4" fill="#2c3e50"/>
    <rect x="38" y="12" width="12" height="8" fill="#1a9b8e" opacity="0.5"/>
    <path d="M 5 15 L 10 10 L 25 10 L 30 15 Z" fill="white" opacity="0.7"/>
  </g>

  <!-- Delivery Van (Center-left) -->
  <g transform="translate(220, 28)">
    <rect x="0" y="12" width="32" height="16" rx="2" fill="white" opacity="0.85"/>
    <path d="M 0 12 L 5 8 L 12 8 L 17 12 Z" fill="white" opacity="0.7"/>
    <rect x="5" y="9" width="8" height="6" fill="#1a9b8e" opacity="0.4"/>
    <circle cx="8" cy="30" r="3.5" fill="#2c3e50"/>
    <circle cx="24" cy="30" r="3.5" fill="#2c3e50"/>
    <rect x="18" y="14" width="12" height="10" fill="white" opacity="0.6"/>
  </g>

  <!-- Container Truck (Center-right) -->
  <g transform="translate(360, 23)">
    <rect x="0" y="17" width="45" height="20" rx="2" fill="white" opacity="0.9"/>
    <rect x="45" y="12" width="22" height="25" rx="2" fill="white" opacity="0.9"/>
    <line x1="10" y1="17" x2="10" y2="37" stroke="#1a9b8e" stroke-width="1" opacity="0.3"/>
    <line x1="20" y1="17" x2="20" y2="37" stroke="#1a9b8e" stroke-width="1" opacity="0.3"/>
    <line x1="30" y1="17" x2="30" y2="37" stroke="#1a9b8e" stroke-width="1" opacity="0.3"/>
    <circle cx="12" cy="39" r="4" fill="#2c3e50"/>
    <circle cx="33" cy="39" r="4" fill="#2c3e50"/>
    <circle cx="57" cy="39" r="4" fill="#2c3e50"/>
    <rect x="48" y="14" width="14" height="10" fill="#1a9b8e" opacity="0.5"/>
  </g>

  <!-- Small Truck (Right) -->
  <g transform="translate(500, 30)">
    <rect x="0" y="12" width="28" height="14" rx="2" fill="white" opacity="0.8"/>
    <path d="M 0 12 L 4 8 L 10 8 L 14 12 Z" fill="white" opacity="0.6"/>
    <rect x="4" y="9" width="6" height="5" fill="#1a9b8e" opacity="0.4"/>
    <circle cx="7" cy="28" r="3" fill="#2c3e50"/>
    <circle cx="21" cy="28" r="3" fill="#2c3e50"/>
  </g>

  <!-- Decorative road lines -->
  <line x1="50" y1="60" x2="100" y2="60" stroke="white" stroke-width="2" opacity="0.3" stroke-dasharray="8,8"/>
  <line x1="150" y1="60" x2="200" y2="60" stroke="white" stroke-width="2" opacity="0.3" stroke-dasharray="8,8"/>
  <line x1="250" y1="60" x2="300" y2="60" stroke="white" stroke-width="2" opacity="0.3" stroke-dasharray="8,8"/>
  <line x1="350" y1="60" x2="400" y2="60" stroke="white" stroke-width="2" opacity="0.3" stroke-dasharray="8,8"/>
  <line x1="450" y1="60" x2="500" y2="60" stroke="white" stroke-width="2" opacity="0.3" stroke-dasharray="8,8"/>

  <!-- Bottom accent line -->
  <line x1="0" y1="78" x2="600" y2="78" stroke="url(#lineGradient)" stroke-width="2"/>
</svg>